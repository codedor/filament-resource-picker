{
    "name": "codedor/filament-resource-picker",
    "description": "This is my package filament-resource-picker",
    "keywords": [
        "codedor",
        "laravel",
        "filament-resource-picker"
    ],
    "homepage": "https://github.com/codedor/filament-resource-picker",
    "license": "MIT",
    "require": {
        "php": "^8.2|^8.3",
        "filament/filament": "^3.0",
        "illuminate/contracts": "^10.0|^11.0",
        "livewire/livewire": "^3.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "larastan/larastan": "^2.0",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Codedor\\FilamentResourcePicker\\": "src",
            "Codedor\\FilamentResourcePicker\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codedor\\FilamentResourcePicker\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedor\\FilamentResourcePicker\\Providers\\FilamentResourcePickerServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-feature/laravel-11": "1.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
